@model IEnumerable<Econocom.Model.Models.Benchmark.Client>

@{
    ViewBag.Title = "Clients";
    Layout = "../Shared/_LayoutAdmin.cshtml";    
}

<h2>Liste des souscriptions en attente</h2>

<table>
    <tr>
        <th>
            Name
        </th>
        <th>
            Groupe
        </th>
        <th>
            Address1
        </th>
        <th>
            Address2
        </th>
        <th>
            Address3
        </th>
        <th>
            Status
        </th>
        <th>
            Country
        </th>
        @*<th>
            Group
        </th>
        <th>
            Sector
        </th>
        <th>
            DateCreated
        </th>
        <th>
            DateModified
        </th>
        <th>
            CreatedBy
        </th>
        <th>
            ModifiedBy
        </th>*@
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Nom)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Groupe)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Adresse.Adresse1)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Adresse.Adresse2)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Adresse.Adresse3)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Adresse.CodePostal)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.RaisonSociale)
        </td>
        @*<td>
            @Html.DisplayFor(modelItem => item.Group.Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Sector.Label)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.DateCreated)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.DateModified)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.CreatedBy)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ModifiedBy)
        </td>*@
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Id })
        </td>
    </tr>
}

</table>
