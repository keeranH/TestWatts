@using Econocom.Model.ViewModel
@using Web.Extension
@model Econocom.Model.ViewModel.ClientBenchmarkViewModel
           
<link href="@Url.Content("~/Content/jquery.mobile-1.3.0.css")" rel="stylesheet" type="text/css" />
<script src="@Url.Content("~/Scripts/jquery-1.8.2.min.js")" type="text/javascript"></script>
<script src="@Url.Content("~/Scripts/jquery.mobile-1.3.0.min.js")" type="text/javascript"></script>


<style>
    .ui-collapsible-inset .ui-collapsible-content
    {
        margin-left: 0;
    }

    .ui-btn-inner {
        display: block;
        position: relative;
        text-overflow: ellipsis;
        overflow: hidden;
        white-space: nowrap;
        zoom: 1;
    }

    .ui-btn-inner 
    {
        padding-right: 0px !important;
        padding-top: 0px !important;
        padding-bottom: 0px !important;
        padding-left: 40px !important;
    }

    input {
        font-size: 1.0em !important;
        background: none;
        padding: 0px;
    }

    .input.ui-input-text {
        background-image: none !important;
        padding: 0 !important;
        margin: 0 !important;
        min-height: 0em !important;
        line-height: 0em !important;
    }

    input.ui-input-text, textarea.ui-input-text {
        background-image: none !important;
        padding: 0 !important;
        margin: 0 !important;
        min-height: 0em !important;
        line-height: 0em !important;
    }

    .ui-collapsible-content {
        padding: 5px 15px;
    }

    .ui-input-search, div.ui-input-text {
        margin: 0px;
    }

    .ui-collapsible-heading {
        font-size: 1.0em!important;
    }
   
            .ui-page {
                width: 1000px !important;
                margin: 0 auto !important;
                position: relative !important;
            }
        
       
    .ui-collapsible-inset {
        margin: 5px 0px;
    }
</style>

<div data-role="page" class="type-interior ui-page ui-body-c ui-page-active">
