        <div class="content-secondary">
            <div>
                <h3>
                    <span style="float: right; margin-right: 8px; width: 136px; margin-top: -25px;">Non Intensive Hrs</span>
                    <span style="float: right; margin-right: 161px; width: 102px; margin-top: -25px;">Intensive Hrs</span>
                    <span style="float: right; margin-right: 266px; width: 128px; margin-top: -25px;">Working Days</span>
                    <span style="float: right; margin-right: 424px; width: 85px; margin-top: -25px;">Total Days</span>
                    <span style="float: right; margin-right: 531px; width: 85px; margin-top: -25px;">Ratio</span>
                    <span style="float: right; margin-right: 662px; width: 85px; margin-top: -25px;">Value</span> 
                </h3>   
               
                @Html.HiddenFor(model => model.ClientId)                   
                @Html.EditorFor(modelItem=>Model.ViewModelDeviceFamilies)
                                                                                      
            </div>
            </div>         
    </div>	       
