@using  myResources = Econocom.Resource.Home.HomeLayout
@using FooterResources= Econocom.Resource.Basic.Footer

<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <title>@ViewBag.Title</title>
    <link href="@Url.Content("~/Content/Site.css")" rel="stylesheet" type="text/css" />
    <link href="@Url.Content("~/Content/AdminCss.css")" rel="stylesheet" type="text/css" />

    <script src="@Url.Content("~/Scripts/jquery-1.5.1.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/modernizr-1.7.min.js")" type="text/javascript"></script>

    <link href="@Url.Content("~/Content/jquery.mobile-1.2.0.min.css")" rel="stylesheet" type="text/css" />
    <script src="@Url.Content("~/Scripts/jquery-1.8.2.min.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/Scripts/jquery.mobile-1.2.0.min.js")" type="text/javascript"></script>

   

</head>
<body>
    <div class="page">

    <header>

       <div id="headerDiv"  data-role="header">
            <div id="logo" >LOGO</div>
            <div id="credentials">
                <div id="logindisplay">
                    @Html.Partial("_LogOnPartial")
                    
             </div>
            </div>
       </div>
    </header>

    <div id="bodyDiv" data-role="content">

    @RenderBody()

        <div id="targetOne" >
            <div id="greenItDiv"><span style="color:greenyellow;" id="idz" class="idz">@Html.ActionLink(@myResources.GreenIT, "Index", "GreenIT")  >></span></div>
        </div>
        <div id="targetTwo">
            <div id="demoDiv"><span style="color:greenyellow; ">@Html.ActionLink(@myResources.Demo, "Demo", "Client") >></span></div>
        </div>
        <div id="targetThree" >
            <div id="informationDiv"><span style="color:greenyellow; ">@myResources.Information  >></span></div>
        </div>
        <div id="targetFour">

            <div id="registrationDiv"><span style="color:greenyellow; ">@Html.ActionLink(@myResources.Registration , "Registration", "Client") >></span></div>

        </div>
    
    </div>

        <div id="footerDiv" data-role="footer">
            <div >
               @FooterResources.FooterInfo
             </div>
        </div>
    </div>
</body>
</html>
