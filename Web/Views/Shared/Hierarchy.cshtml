@model IEnumerable<Econocom.Model.Models.Benchmark.DeviceFamily>
           
<link href="@Url.Content("~/Content/jquery.mobile-1.3.0.css")" rel="stylesheet" type="text/css" />
<script src="@Url.Content("~/Scripts/jquery-1.8.2.min.js")" type="text/javascript"></script>
<script src="@Url.Content("~/Scripts/jquery.mobile-1.3.0.min.js")" type="text/javascript"></script>

<link href="@Url.Content("~/Content/CalculatorCss.css")" rel="stylesheet" type="text/css" />

<script type="text/javascript">
    $('.divC').bind('expand', function () {
        alert('Expanded');
    }).bind('collapse', function () {
        alert('Collapsed');
    });

   
</script>

<style>
.ui-collapsible-inset .ui-collapsible-content
{
margin-left: 0;
}

.ui-btn-inner {
display: block;
position: relative;
text-overflow: ellipsis;
overflow: hidden;
white-space: nowrap;
zoom: 1;
}

.ui-collapsible-inset {
margin: 0;
}
</style>


<body>
    <div data-role="page" style="border:solid 1px black;">
        <div data-role="content" style="border:solid 1px blue;">
            <div data-role="collapsible" id="my-collaspible" class="divC ui-collapsible-heading" style="border:solid 1px yellow;">
                <h3>Banque<span style="float:right;border:solid 1px red">1234.56</span><span style="float:right; margin-right:80px;border:solid 1px blue">5555</span></h3>
            
                @foreach (var family in Model)
                {
                    <div data-role="collapsible" id="my-collaspible1" class="divC"> 
                        <h3>@Html.DisplayFor(modelItem => family.Label)<span style="float: right; border: solid 1px red">1234.56</span><span style="float: right; margin-right: 80px; border: solid 1px blue">5555</span></h3>										
                    
                        @foreach (var category in family.Categories)
                        {   
                            <div data-role="collapsible" id="my-collaspible1" class="divC">
                                <h3>@Html.DisplayFor(modelItem => category.Label)<span style="float: right; border: solid 1px red">1234.56</span><span style="float: right; margin-right: 80px; border: solid 1px blue">5555</span></h3>										
                        
                                @foreach (var type in category.DeviceTypes)
                                { 
                                    <div data-role="collapsible" id="my-collaspible1" class="divC">
                                        <h3>@Html.DisplayFor(modelItem => type.Label)<span style="float: right; border: solid 1px red">1234.56</span><span style="float: right; margin-right: 80px; border: solid 1px blue">5555</span></h3>										
                                 
                                        <div data-role="collapsible" id="my-collaspible1" class="divC">
                                            <h3>Entrée de gamme<span style="float: right; border: solid 1px red">1234.56</span><span style="float: right; margin-right: 80px; border: solid 1px blue">5555</span></h3>										
                                            <p>Moins d'un an<span style="float:right;border:solid 1px red">1234.56</span><span style="float:right; margin-right:80px;border:solid 1px blue">5555</span></p>
                                            <p>De 1 à 3 ans<span style="float:right;border:solid 1px red">1234.56</span><span style="float:right; margin-right:80px;border:solid 1px blue">5555</span></p>
                                            <p>De 3 à 5 ans<span style="float:right;border:solid 1px red">1234.56</span><span style="float:right; margin-right:80px;border:solid 1px blue">5555</span></p>
                                            <p>Plus de 5 ans<span style="float:right;border:solid 1px red">1234.56</span><span style="float:right; margin-right:80px;border:solid 1px blue">5555</span></p>
                                        </div>
                                        <div data-role="collapsible" id="my-collaspible1" class="divC">
                                            <h3>Milieur de gamme<span style="float: right; border: solid 1px red">1234.56</span><span style="float: right; margin-right: 80px; border: solid 1px blue">5555</span></h3>
                                            <p>Moins d'un an<span style="float:right;border:solid 1px red">1234.56</span><span style="float:right; margin-right:80px;border:solid 1px blue">5555</span></p>
                                            <p>De 1 à 3 ans<span style="float:right;border:solid 1px red">1234.56</span><span style="float:right; margin-right:80px;border:solid 1px blue">5555</span></p>
                                            <p>De 3 à 5 ans<span style="float:right;border:solid 1px red">1234.56</span><span style="float:right; margin-right:80px;border:solid 1px blue">5555</span></p>
                                            <p>Plus de 5 ans<span style="float:right;border:solid 1px red">1234.56</span><span style="float:right; margin-right:80px;border:solid 1px blue">5555</span></p>										
                                        </div>
                                        <div data-role="collapsible" id="my-collaspible1" class="divC">
                                            <h3>Haut de gamme<span style="float: right; border: solid 1px red">1234.56</span><span style="float: right; margin-right: 80px; border: solid 1px blue">5555</span></h3>
                                            <p>Moins d'un an<span style="float:right;border:solid 1px red">1234.56</span><span style="float:right; margin-right:80px;border:solid 1px blue">5555</span></p>
                                            <p>De 1 à 3 ans<span style="float:right;border:solid 1px red">1234.56</span><span style="float:right; margin-right:80px;border:solid 1px blue">5555</span></p>
                                            <p>De 3 à 5 ans<span style="float:right;border:solid 1px red">1234.56</span><span style="float:right; margin-right:80px;border:solid 1px blue">5555</span></p>
                                            <p>Plus de 5 ans<span style="float:right;border:solid 1px red">1234.56</span><span style="float:right; margin-right:80px;border:solid 1px blue">5555</span></p>
                                        </div>
                                    </div>
                                }
                            </div>
                        }
                
                    </div>
                }
            
            </div>
        </div>
        <div data-role="content" style="padding:0px;">		
            <div data-role="collapsible" id="my-collaspible" class="divC" style="padding:0px;">
                <h3>Total<span style="float:right;border:solid 1px red">1234.56</span><span style="float:right; margin-right:80px;border:solid 1px blue">5555</span></h3>
            </div>		
        </div>
    </div>

    <div>
        @Html.Partial("Family", Model)
    </div>

</body>