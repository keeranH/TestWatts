@model IEnumerable<Econocom.Model.Models.Benchmark.ClientDetail>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table>
    <tr>
        <th>
            NumberOfEmployees
        </th>
        <th>
            RatioDeviceEmployee
        </th>
        <th>
            TurnOver
        </th>
        <th>
            PriceOfKWatt
        </th>
        <th>
            TotalElectricityBill
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.NumberOfEmployees)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.RatioDeviceEmployee)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.TurnOver)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.PriceOfKWatt)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.TotalElectricityBill)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Id }) |
            @Html.ActionLink("Details", "Details", new { id=item.Id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Id })
        </td>

        <div class="editor-field">
            @Html.DropDownList("ReportTypes", String.Empty)
          
        </div>

    </tr>
}

</table>
