@model Econocom.Model.ViewModel.ContactSouscriptionViewModel
@using Econocom.Resource;

@{
    ViewBag.Title = "ContactTemplate";
    Layout = "~/Views/Shared/Souscription_Layout/SouscripteurSouscription_Layout.cshtml";
    
    <style type="text/css">
        .souscripteurLabelFor {
            font-family: "Times New Roman";
        }

        .displayedValue {
            width: 320px;
            background-color: #EDFFB5;
            height: 20px;
        }
    </style>
}

@section option2{
    <div align="left" style="width: 200px; float: left">
       <span style="font-size: 1.5em">@Traduction.Souscripteur</span>
    </div>
}

@Html.HiddenFor(model => model.ClientId)

@section Nom2{
    @Html.LabelFor(model => model.Nom, Traduction.Nom, new { @class = "souscripteurLabelFor" }) 
    <div  class="displayedValue">@Html.DisplayFor(model => model.Nom)</div>
}

@section Fonction2{
    @Html.LabelFor(model => model.Fonction, Traduction.Fonction, new { @class = "souscripteurLabelFor" }) 
    <div  class="displayedValue">@Html.DisplayFor(model => Model.Fonction, new { disabled = "true" })</div>
}

@section AdresseMail2{
    @Html.LabelFor(model => model.Email, Traduction.AddresseMail, new { @class = "souscripteurLabelFor" }) 
   <div  class="displayedValue"> @Html.DisplayFor(model => Model.Email, new { disabled = "true" })</div>
}

@section Prenom2{
    @Html.LabelFor(model => model.Prenom, Traduction.Prenom, new { @class = "souscripteurLabelFor" }) 
    <div  class="displayedValue">@Html.DisplayFor(model => Model.Prenom, new { disabled = "true" })</div>
}

@section Telephone2{
    @Html.LabelFor(model => model.Telephone, Traduction.Telephone, new { @class = "souscripteurLabelFor" })
    <div  class="displayedValue">@Html.DisplayFor(model => Model.Telephone, new { disabled = "true" })</div>
}

